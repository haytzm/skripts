		set last spawned Zombie's health to 25
		add "{NoAI:1}" to nbt of last spawned zombie
		set name of last spawned zombie to "project 0"
#reset the combat level of the select player you want
command /resetcombat <player>:
	permission: op
	trigger:
		set {combatlevel::%arg-1%} to 0
		set {combatxp::%arg%} to 0
		send "&a&lYou have reset the comabat level of %arg-1%" to player
		send "&a&lYour combat level has been reset by %player%" to arg-1
