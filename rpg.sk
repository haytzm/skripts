command /clearlag:
  permission: clearlag
  permission message: &6[ERROR] &rYou do not have permission to execute this command!
  trigger:
    broadcast "&7[&6Spirant&7] &4%player% &cis Clearing all entities in 5 seconds!"
    wait 5 seconds
    execute console command "/execute run kill @e[type=!player]"
#hologram funtion for the amount of coins yoy get on killing a monster
function createHolo(l: Location, t: Text, length: Timespan):
    create hologram "%{_t}%" at {_l} for {_length}
#spawns a boss every 5 minutes this is the begginer boss so it isnt that strong or good to kill
every 5 minutes in "spirant":
	if {golem} = 0:
		set {golem} to 1
		spawn iron golem at location at (-149, 45, -261) 
		set max health of last spawned iron golem to 100
		set health of last spawned iron golem to 100
		give last spawned iron golem netherite axe of sharpness 2
		set name of last spawned iron golem to "&5&lEnchanted protecter"
#if you are testing and want to spawn the boss this command will do so
command /irong:
	permission: op
	trigger:
		set {golem} to 1
		spawn iron golem at location at (-149, 45, -261) 
		set max health of last spawned iron golem to 100
		set health of last spawned iron golem to 100
		give last spawned iron golem netherite axe of sharpness 2
		set name of last spawned iron golem to "&5&lEnchanted protecter"
#death sequence of the boss
on death of iron golem:
	if name of victim is "&5&lEnchanted protecter":
		set {golem} to 0
		cancel drops
		broadcast "&e------------------------------"
		broadcast "&5&lEnchanted protecter &6Is down"
		broadcast "&e--------------------------------"
		if {5mxpbooster::%attacker%} = 1:
			add 50 to {combatxp::%attacker%}
		else:
			add 25 to {combatxp::%attacker%}
		chance of 80%:
			add 1000 to attacker's balance
			broadcast "&b%attacker% won 1000 coins"
			give 5 of iron ingot to attacker 
			execute console command "execute at %attacker% run playsound minecraft:entity.player.levelup player %attacker%"
#testing dummy    
on death of pig:
	if name of victim is "testing dummy":
		spawn 1 of pig at location of victim
		add "{NoAI:1}" to nbt of last spawned pig
		set name of last spawned pig to "testing dummy"
		set last spawned pig's max health to 500000000
		set last spawned pig's health to 50000000
		apply resistance 10000 to last spawned pig for 100000000 days
on death of pig:
	if display name of victim is "testing dummy":
		cancel drops
		chance of 20%:
			give raw porkchop to attacker
on damage:
		wait 1 ticks
		set action bar of attacker to "&8[&c%victim's health% &8/ &c%victim's max health%&8]"
		set {activedamage::%attacker%} to 1
		wait 5 seconds
		set {activedamage::%attacker%} to 0
on any movement:
	if {activedamage::%player%} = 0:
		set action bar of player to "&a%player's health%/%player's max health%❤"
on death of pig:
    if display name of victim is "testing dummy":
        loop blocks in radius 2 around head of victim:
            add location of loop-block to {_blocks::*}
        set {_bl} to random element of {_blocks::*}
        createHolo({_bl}, "&6+ &e0 coins", 25 ticks)
#the start of project 0 the zombie
on death of zombie:
	if name of victim is "project 0":
